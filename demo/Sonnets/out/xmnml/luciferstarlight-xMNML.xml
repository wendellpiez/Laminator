   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r34_tercet r35_line r38_phr"
         off="536"
         ext="9">and sank.</text>
   <end gi="phr" rID="r38_phr" off="545" ext="9"/>
   <end gi="line" rID="r35_line" off="545" ext="51"/>
