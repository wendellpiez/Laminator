   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r27_tercet r31_line r33_phr"
         off="476"
         ext="17">and at the stars,</text>
   <end gi="phr" rID="r33_phr" off="493" ext="17"/>
   <end gi="line" rID="r31_line" off="493" ext="45"/>
   <end gi="tercet" rID="r27_tercet" off="493" ext="136"/>
