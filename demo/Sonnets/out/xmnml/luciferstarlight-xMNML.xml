   <start gi="phr" rID="r28_phr" off="357" ext="90"/>
   <start gi="line" rID="r29_line" off="357" ext="50"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r27_tercet r28_phr r29_line"
         off="357"
         ext="50">Soaring through wider zones that pricked his scars</text>
   <end gi="line" rID="r29_line" off="407" ext="50"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r27_tercet r28_phr"
         off="407"
         ext="1">
</text>
   <start gi="line" rID="r30_line" off="408" ext="39"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r27_tercet r28_phr r30_line"
         off="408"
         ext="39">With memory of the old revolt from Awe,</text>
   <end gi="line" rID="r30_line" off="447" ext="39"/>
   <end gi="phr" rID="r28_phr" off="447" ext="90"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r27_tercet"
         off="447"
         ext="1">
</text>
   <start gi="line" rID="r31_line" off="448" ext="45"/>
