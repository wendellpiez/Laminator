   <text cf="r01_sonneteer r02_sonnet r25_sestet r34_tercet r39_s r40_line r41_phr"
         off="546"
         ext="33">Around the ancient track marched,</text>
   <end gi="phr" rID="r41_phr" off="579" ext="33"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r34_tercet r39_s r40_line"
