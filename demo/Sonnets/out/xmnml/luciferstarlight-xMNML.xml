   <text cf="r01_sonneteer r02_sonnet r03_octave r04_quatrain r05_line r06_s r07_phr"
         off="1"
         ext="41">On a starred night Prince Lucifer uprose.</text>
   <end gi="phr" rID="r07_phr" off="42" ext="41"/>
   <end gi="s" rID="r06_s" off="42" ext="41"/>
   <end gi="line" rID="r05_line" off="42" ext="41"/>
