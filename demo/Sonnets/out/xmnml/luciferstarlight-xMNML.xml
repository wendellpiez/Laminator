   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r34_tercet r35_line r37_phr"
         off="525"
         ext="10">he looked,</text>
   <end gi="phr" rID="r37_phr" off="535" ext="10"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r34_tercet r35_line"
         off="535"
         ext="1"> </text>
