   <start gi="phr" rID="r20_phr" off="225" ext="40"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r18_s r19_line r20_phr"
         off="225"
         ext="40">And now upon his western wing he leaned,</text>
   <end gi="phr" rID="r20_phr" off="265" ext="40"/>
   <end gi="line" rID="r19_line" off="265" ext="40"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r18_s"
         off="265"
         ext="1">
</text>
   <start gi="line" rID="r21_line" off="266" ext="46"/>
   <start gi="phr" rID="r22_phr" off="266" ext="46"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r18_s r21_line r22_phr"
         off="266"
         ext="46">Now his huge bulk o'er Afric's sands careened,</text>
   <end gi="phr" rID="r22_phr" off="312" ext="46"/>
   <end gi="line" rID="r21_line" off="312" ext="46"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r18_s"
         off="312"
         ext="1">
</text>
   <start gi="line" rID="r23_line" off="313" ext="43"/>
   <start gi="phr" rID="r24_phr" off="313" ext="43"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r18_s r23_line r24_phr"
         off="313"
         ext="43">Now the black planet shadowed Arctic snows.</text>
   <end gi="phr" rID="r24_phr" off="356" ext="43"/>
   <end gi="line" rID="r23_line" off="356" ext="43"/>
   <end gi="s" rID="r18_s" off="356" ext="131"/>
