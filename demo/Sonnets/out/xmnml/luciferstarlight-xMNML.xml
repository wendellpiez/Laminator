   <start gi="phr" rID="r36_phr" off="494" ext="30"/>
   <text cf="r01_sonneteer r02_sonnet r25_sestet r26_s r34_tercet r35_line r36_phr"
         off="494"
         ext="30">Which are the brain of heaven,</text>
   <end gi="phr" rID="r36_phr" off="524" ext="30"/>
