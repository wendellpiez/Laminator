   <start gi="s" rID="r16_s" off="179" ext="45"/>
   <start gi="phr" rID="r17_phr" off="179" ext="45"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain r15_line r16_s r17_phr"
         off="179"
         ext="45">Poor prey to his hot fit of pride were those.</text>
   <end gi="phr" rID="r17_phr" off="224" ext="45"/>
   <end gi="s" rID="r16_s" off="224" ext="45"/>
   <end gi="line" rID="r15_line" off="224" ext="45"/>
   <text cf="r01_sonneteer r02_sonnet r03_octave r14_quatrain"
         off="224"
         ext="1">
</text>
   <start gi="s" rID="r18_s" off="225" ext="131"/>
