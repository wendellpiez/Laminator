<?xml version="1.0" encoding="UTF-8"?>
<LMNL xmlns="http://wendellpiez.com/ns/xMNML">
   <start gi="excerpt" rID="r1_excerpt" off="0" ext="123">
      <annotation gi="author">Robert Frost</annotation>
      <annotation gi="date">1915</annotation>
      <annotation gi="title">The Housekeeper</annotation>
   </start>
   <text cf="r1_excerpt" off="0" ext="1">
</text>
   <start gi="s" rID="r2_s" off="1" ext="50"/>
   <start gi="l" rID="r3_l" off="1" ext="33">
      <annotation gi="n">144</annotation>
   </start>
   <text cf="r1_excerpt r2_s r3_l" off="1" ext="33">He manages to keep the upper hand</text>
   <end gi="l" rID="r3_l" off="34" ext="33"/>
   <text cf="r1_excerpt r2_s" off="34" ext="1">
</text>
   <start gi="l" rID="r4_l" off="35" ext="43">
      <annotation gi="n">145</annotation>
   </start>
   <text cf="r1_excerpt r2_s r4_l" off="35" ext="16">On his own farm.</text>
   <end gi="s" rID="r2_s" off="51" ext="50"/>
   <text cf="r1_excerpt r4_l" off="51" ext="1"> </text>
   <start gi="s" rID="r5_s" off="52" ext="10"/>
   <text cf="r1_excerpt r4_l r5_s" off="52" ext="10">He's boss.</text>
   <end gi="s" rID="r5_s" off="62" ext="10"/>
   <text cf="r1_excerpt r4_l" off="62" ext="1"> </text>
   <start gi="s" rID="r6_s" off="63" ext="59"/>
   <text cf="r1_excerpt r4_l r6_s" off="63" ext="15">But as to hens:</text>
   <end gi="l" rID="r4_l" off="78" ext="43"/>
   <text cf="r1_excerpt r6_s" off="78" ext="1">
</text>
   <start gi="l" rID="r7_l" off="79" ext="43">
      <annotation gi="n">146</annotation>
   </start>
   <text cf="r1_excerpt r6_s r7_l" off="79" ext="43">We fence our flowers in and the hens range.</text>
   <end gi="l" rID="r7_l" off="122" ext="43"/>
   <end gi="s" rID="r6_s" off="122" ext="59"/>
   <text cf="r1_excerpt" off="122" ext="1">
</text>
   <end gi="excerpt" rID="r1_excerpt" off="123" ext="123"/>
   <text off="123" ext="1">
</text>
