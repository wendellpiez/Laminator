<LMNL xmlns="http://wendellpiez.com/ns/xMNML">
   <start gi="excerpt" rID="r01_excerpt" off="0" ext="1105">
      <annotation gi="title">Paradise Lost I 1-26</annotation>
      <annotation gi="book">I</annotation>
      <annotation gi="ll">1-26</annotation>
      <annotation gi="author">John Milton</annotation>
   </start>
   <text cf="r01_excerpt" off="0" ext="1">
</text>
   <start gi="l" rID="r02_l" off="1" ext="42">
      <annotation gi="n">Bk.1.1</annotation>
   </start>
   <start gi="s" rID="r03_s" off="1" ext="670"/>
   <start gi="phr" rID="r04_phr" off="1" ext="28"/>
   <text cf="r01_excerpt r02_l r03_s r04_phr" off="1" ext="28">Of Man's first disobedience,</text>
   <end gi="phr" rID="r04_phr" off="29" ext="28"/>
   <text cf="r01_excerpt r02_l r03_s" off="29" ext="1"> </text>
   <start gi="phr" rID="r05_phr" off="30" ext="85"/>
   <text cf="r01_excerpt r02_l r03_s r05_phr" off="30" ext="4">and </text>
   <start gi="np" id="1" rID="r06_np" off="34" ext="32"/>
   <text cf="r01_excerpt r02_l r03_s r05_phr r06_np" off="34" ext="9">the fruit</text>
   <end gi="l" rID="r02_l" off="43" ext="42"/>
   <text cf="r01_excerpt r03_s r05_phr r06_np" off="43" ext="1">
</text>
   <start gi="l" rID="r07_l" off="44" ext="41">
      <annotation gi="n">Bk.1.2</annotation>
   </start>
   <text cf="r01_excerpt r03_s r05_phr r06_np r07_l" off="44" ext="3">Of </text>
   <start gi="np" id="2" rID="r08_np" off="47" ext="104"/>
   <text cf="r01_excerpt r03_s r05_phr r06_np r07_l r08_np"
         off="47"
         ext="19">that forbidden tree</text>
   <end gi="np" id="1" rID="r06_np" off="66" ext="32"/>
   <text cf="r01_excerpt r03_s r05_phr r07_l r08_np" off="66" ext="19"> whose mortal taste</text>
   <end gi="l" rID="r07_l" off="85" ext="41"/>
   <text cf="r01_excerpt r03_s r05_phr r08_np" off="85" ext="1">
</text>
   <start gi="l" rID="r09_l" off="86" ext="46">
      <annotation gi="n">Bk.1.3</annotation>
   </start>
   <text cf="r01_excerpt r03_s r05_phr r08_np r09_l" off="86" ext="29">Brought death into the World,</text>
   <end gi="phr" rID="r05_phr" off="115" ext="85"/>
   <text cf="r01_excerpt r03_s r08_np r09_l" off="115" ext="1"> </text>
   <start gi="phr" rID="r10_phr" off="116" ext="16"/>
   <text cf="r01_excerpt r03_s r08_np r09_l r10_phr" off="116" ext="16">and all our woe,</text>
   <end gi="phr" rID="r10_phr" off="132" ext="16"/>
   <end gi="l" rID="r09_l" off="132" ext="46"/>
   <text cf="r01_excerpt r03_s r08_np" off="132" ext="1">
</text>
   <start gi="l" rID="r11_l" off="133" ext="39">
      <annotation gi="n">Bk.1.4</annotation>
   </start>
   <start gi="phr" rID="r12_phr" off="133" ext="18"/>
   <text cf="r01_excerpt r03_s r08_np r11_l r12_phr" off="133" ext="18">With loss of Eden,</text>
   <end gi="np" id="2" rID="r08_np" off="151" ext="104"/>
   <end gi="phr" rID="r12_phr" off="151" ext="18"/>
   <text cf="r01_excerpt r03_s r11_l" off="151" ext="1"> </text>
   <start gi="phr" rID="r13_phr" off="152" ext="32"/>
   <text cf="r01_excerpt r03_s r11_l r13_phr" off="152" ext="20">till one greater Man</text>
   <end gi="l" rID="r11_l" off="172" ext="39"/>
   <text cf="r01_excerpt r03_s r13_phr" off="172" ext="1">
</text>
   <start gi="l" rID="r14_l" off="173" ext="41">
      <annotation gi="n">Bk.1.5</annotation>
   </start>
   <text cf="r01_excerpt r03_s r13_phr r14_l" off="173" ext="11">Restore us,</text>
   <end gi="phr" rID="r13_phr" off="184" ext="32"/>
   <text cf="r01_excerpt r03_s r14_l" off="184" ext="1"> </text>
   <start gi="phr" rID="r15_phr" off="185" ext="29"/>
   <text cf="r01_excerpt r03_s r14_l r15_phr" off="185" ext="29">and regain the blissful seat,</text>
   <end gi="phr" rID="r15_phr" off="214" ext="29"/>
   <end gi="l" rID="r14_l" off="214" ext="41"/>
   <text cf="r01_excerpt r03_s" off="214" ext="1">
</text>
   <start gi="l" rID="r16_l" off="215" ext="44">
      <annotation gi="n">Bk.1.6</annotation>
   </start>
   <start gi="phr" rID="r17_phr" off="215" ext="5"/>
   <text cf="r01_excerpt r03_s r16_l r17_phr" off="215" ext="5">Sing,</text>
   <end gi="phr" rID="r17_phr" off="220" ext="5"/>
   <text cf="r01_excerpt r03_s r16_l" off="220" ext="1"> </text>
   <start gi="np" id="3" rID="r18_np" off="221" ext="88"/>
   <start gi="phr" rID="r19_phr" off="221" ext="14"/>
   <text cf="r01_excerpt r03_s r16_l r18_np r19_phr" off="221" ext="14">Heavenly Muse,</text>
   <end gi="phr" rID="r19_phr" off="235" ext="14"/>
   <text cf="r01_excerpt r03_s r16_l r18_np" off="235" ext="1"> </text>
   <start gi="phr" rID="r20_phr" off="236" ext="5"/>
   <text cf="r01_excerpt r03_s r16_l r18_np r20_phr" off="236" ext="5">that,</text>
   <end gi="phr" rID="r20_phr" off="241" ext="5"/>
   <text cf="r01_excerpt r03_s r16_l r18_np" off="241" ext="1"> </text>
   <start gi="phr" rID="r21_phr" off="242" ext="26"/>
   <text cf="r01_excerpt r03_s r16_l r18_np r21_phr" off="242" ext="17">on the secret top</text>
   <end gi="l" rID="r16_l" off="259" ext="44"/>
   <text cf="r01_excerpt r03_s r18_np r21_phr" off="259" ext="1">
</text>
   <start gi="l" rID="r22_l" off="260" ext="35">
      <annotation gi="n">Bk.1.7</annotation>
   </start>
   <text cf="r01_excerpt r03_s r18_np r21_phr r22_l" off="260" ext="8">Of Oreb,</text>
   <end gi="phr" rID="r21_phr" off="268" ext="26"/>
   <text cf="r01_excerpt r03_s r18_np r22_l" off="268" ext="1"> </text>
   <start gi="phr" rID="r23_phr" off="269" ext="12"/>
   <text cf="r01_excerpt r03_s r18_np r22_l r23_phr" off="269" ext="12">or of Sinai,</text>
   <end gi="phr" rID="r23_phr" off="281" ext="12"/>
   <text cf="r01_excerpt r03_s r18_np r22_l" off="281" ext="1"> </text>
   <start gi="phr" rID="r24_phr" off="282" ext="122"/>
   <text cf="r01_excerpt r03_s r18_np r22_l r24_phr" off="282" ext="13">didst inspire</text>
   <end gi="l" rID="r22_l" off="295" ext="35"/>
   <text cf="r01_excerpt r03_s r18_np r24_phr" off="295" ext="1">
</text>
   <start gi="l" rID="r25_l" off="296" ext="46">
      <annotation gi="n">Bk.1.8</annotation>
   </start>
   <start gi="np" id="4" rID="r26_np" off="296" ext="108"/>
   <text cf="r01_excerpt r03_s r18_np r24_phr r25_l r26_np"
         off="296"
         ext="13">That Shepherd</text>
   <end gi="np" id="3" rID="r18_np" off="309" ext="88"/>
   <text cf="r01_excerpt r03_s r24_phr r25_l r26_np" off="309" ext="33"> who first taught the chosen seed</text>
   <end gi="l" rID="r25_l" off="342" ext="46"/>
   <text cf="r01_excerpt r03_s r24_phr r26_np" off="342" ext="1">
</text>
   <start gi="l" rID="r27_l" off="343" ext="42">
      <annotation gi="n">Bk.1.9</annotation>
   </start>
   <text cf="r01_excerpt r03_s r24_phr r26_np r27_l" off="343" ext="42">In the beginning how the heavens and earth</text>
   <end gi="l" rID="r27_l" off="385" ext="42"/>
   <text cf="r01_excerpt r03_s r24_phr r26_np" off="385" ext="1">
</text>
   <start gi="l" rID="r28_l" off="386" ext="35">
      <annotation gi="n">Bk.1.10</annotation>
   </start>
   <text cf="r01_excerpt r03_s r24_phr r26_np r28_l" off="386" ext="18">Rose out of Chaos:</text>
   <end gi="phr" rID="r24_phr" off="404" ext="122"/>
   <end gi="np" id="4" rID="r26_np" off="404" ext="108"/>
   <text cf="r01_excerpt r03_s r28_l" off="404" ext="1"> </text>
   <start gi="phr" rID="r29_phr" off="405" ext="3"/>
   <text cf="r01_excerpt r03_s r28_l r29_phr" off="405" ext="3">or,</text>
   <end gi="phr" rID="r29_phr" off="408" ext="3"/>
   <text cf="r01_excerpt r03_s r28_l" off="408" ext="1"> </text>
   <start gi="phr" rID="r30_phr" off="409" ext="31"/>
   <text cf="r01_excerpt r03_s r28_l r30_phr" off="409" ext="12">if Sion hill</text>
   <end gi="l" rID="r28_l" off="421" ext="35"/>
   <text cf="r01_excerpt r03_s r30_phr" off="421" ext="1">
</text>
   <start gi="l" rID="r31_l" off="422" ext="48">
      <annotation gi="n">Bk.1.11</annotation>
   </start>
   <text cf="r01_excerpt r03_s r30_phr r31_l" off="422" ext="18">Delight thee more,</text>
   <end gi="phr" rID="r30_phr" off="440" ext="31"/>
   <text cf="r01_excerpt r03_s r31_l" off="440" ext="1"> </text>
   <start gi="phr" rID="r32_phr" off="441" ext="56"/>
   <text cf="r01_excerpt r03_s r31_l r32_phr" off="441" ext="4">and </text>
   <start gi="np" id="5" rID="r33_np" off="445" ext="52"/>
   <text cf="r01_excerpt r03_s r31_l r32_phr r33_np" off="445" ext="25">Siloa's brook that flowed</text>
   <end gi="l" rID="r31_l" off="470" ext="48"/>
   <text cf="r01_excerpt r03_s r32_phr r33_np" off="470" ext="1">
</text>
   <start gi="l" rID="r34_l" off="471" ext="35">
      <annotation gi="n">Bk.1.12</annotation>
   </start>
   <text cf="r01_excerpt r03_s r32_phr r33_np r34_l" off="471" ext="26">Fast by the oracle of God,</text>
   <end gi="phr" rID="r32_phr" off="497" ext="56"/>
   <end gi="np" id="5" rID="r33_np" off="497" ext="52">
      <annotation gi="ref">John 9</annotation>
   </end>
   <text cf="r01_excerpt r03_s r34_l" off="497" ext="1"> </text>
   <start gi="phr" rID="r35_phr" off="498" ext="47"/>
   <text cf="r01_excerpt r03_s r34_l r35_phr" off="498" ext="8">I thence</text>
   <end gi="l" rID="r34_l" off="506" ext="35"/>
   <text cf="r01_excerpt r03_s r35_phr" off="506" ext="1">
</text>
   <start gi="l" rID="r36_l" off="507" ext="38">
      <annotation gi="n">Bk.1.13</annotation>
   </start>
   <text cf="r01_excerpt r03_s r35_phr r36_l" off="507" ext="18">Invoke thy aid to </text>
   <start gi="np" id="6" rID="r37_np" off="525" ext="146"/>
   <text cf="r01_excerpt r03_s r35_phr r36_l r37_np" off="525" ext="20">my adventurous song,</text>
   <end gi="phr" rID="r35_phr" off="545" ext="47"/>
   <end gi="l" rID="r36_l" off="545" ext="38"/>
   <text cf="r01_excerpt r03_s r37_np" off="545" ext="1">
</text>
   <start gi="l" rID="r38_l" off="546" ext="42">
      <annotation gi="n">Bk.1.14</annotation>
   </start>
   <start gi="phr" rID="r39_phr" off="546" ext="66"/>
   <text cf="r01_excerpt r03_s r37_np r38_l r39_phr" off="546" ext="42">That with no middle flight intends to soar</text>
   <end gi="l" rID="r38_l" off="588" ext="42"/>
   <text cf="r01_excerpt r03_s r37_np r39_phr" off="588" ext="1">
</text>
   <start gi="l" rID="r40_l" off="589" ext="40">
      <annotation gi="n">Bk.1.15</annotation>
   </start>
   <text cf="r01_excerpt r03_s r37_np r39_phr r40_l" off="589" ext="23">Above th' Aonian mount,</text>
   <end gi="phr" rID="r39_phr" off="612" ext="66"/>
   <text cf="r01_excerpt r03_s r37_np r40_l" off="612" ext="1"> </text>
   <start gi="phr" rID="r41_phr" off="613" ext="58"/>
   <text cf="r01_excerpt r03_s r37_np r40_l r41_phr" off="613" ext="16">while it pursues</text>
   <end gi="l" rID="r40_l" off="629" ext="40"/>
   <text cf="r01_excerpt r03_s r37_np r41_phr" off="629" ext="1">
</text>
   <start gi="l" rID="r42_l" off="630" ext="41">
      <annotation gi="n">Bk.1.16</annotation>
   </start>
   <start gi="np" id="7" rID="r43_np" off="630" ext="41"/>
   <text cf="r01_excerpt r03_s r37_np r41_phr r42_l r43_np"
         off="630"
         ext="41">Things unattempted yet in prose or rhyme.</text>
   <end gi="np" id="7" rID="r43_np" off="671" ext="41"/>
   <end gi="np" id="6" rID="r37_np" off="671" ext="146"/>
   <end gi="phr" rID="r41_phr" off="671" ext="58"/>
   <end gi="s" rID="r03_s" off="671" ext="670"/>
   <end gi="l" rID="r42_l" off="671" ext="41"/>
   <text cf="r01_excerpt" off="671" ext="1">
</text>
   <start gi="l" rID="r44_l" off="672" ext="44">
      <annotation gi="n">Bk.1.17</annotation>
   </start>
   <start gi="s" rID="r45_s" off="672" ext="432"/>
   <start gi="phr" rID="r46_phr" off="672" ext="17"/>
   <text cf="r01_excerpt r44_l r45_s r46_phr" off="672" ext="17">And chiefly thou,</text>
   <end gi="phr" rID="r46_phr" off="689" ext="17"/>
   <text cf="r01_excerpt r44_l r45_s" off="689" ext="1"> </text>
   <start gi="np" id="8" rID="r47_np" off="690" ext="73"/>
   <start gi="phr" rID="r48_phr" off="690" ext="9"/>
   <text cf="r01_excerpt r44_l r45_s r47_np r48_phr" off="690" ext="9">O Spirit,</text>
   <end gi="phr" rID="r48_phr" off="699" ext="9"/>
   <text cf="r01_excerpt r44_l r45_s r47_np" off="699" ext="1"> </text>
   <start gi="phr" rID="r49_phr" off="700" ext="63"/>
   <text cf="r01_excerpt r44_l r45_s r47_np r49_phr" off="700" ext="16">that dost prefer</text>
   <end gi="l" rID="r44_l" off="716" ext="44"/>
   <text cf="r01_excerpt r45_s r47_np r49_phr" off="716" ext="1">
</text>
   <start gi="l" rID="r50_l" off="717" ext="46">
      <annotation gi="n">Bk.1.18</annotation>
   </start>
   <text cf="r01_excerpt r45_s r47_np r49_phr r50_l" off="717" ext="46">Before all temples th' upright heart and pure,</text>
   <end gi="np" id="8" rID="r47_np" off="763" ext="73"/>
   <end gi="phr" rID="r49_phr" off="763" ext="63"/>
   <end gi="l" rID="r50_l" off="763" ext="46"/>
   <text cf="r01_excerpt r45_s" off="763" ext="1">
</text>
   <start gi="l" rID="r51_l" off="764" ext="50">
      <annotation gi="n">Bk.1.19</annotation>
   </start>
   <start gi="phr" rID="r52_phr" off="764" ext="12"/>
   <text cf="r01_excerpt r45_s r51_l r52_phr" off="764" ext="12">Instruct me,</text>
   <end gi="phr" rID="r52_phr" off="776" ext="12"/>
   <text cf="r01_excerpt r45_s r51_l" off="776" ext="1"> </text>
   <start gi="phr" rID="r53_phr" off="777" ext="17"/>
   <text cf="r01_excerpt r45_s r51_l r53_phr" off="777" ext="17">for Thou know'st;</text>
   <end gi="phr" rID="r53_phr" off="794" ext="17"/>
   <text cf="r01_excerpt r45_s r51_l" off="794" ext="1"> </text>
   <start gi="phr" rID="r54_phr" off="795" ext="33"/>
   <text cf="r01_excerpt r45_s r51_l r54_phr" off="795" ext="19">Thou from the first</text>
   <end gi="l" rID="r51_l" off="814" ext="50"/>
   <text cf="r01_excerpt r45_s r54_phr" off="814" ext="1">
</text>
   <start gi="l" rID="r55_l" off="815" ext="47">
      <annotation gi="n">Bk.1.20</annotation>
   </start>
   <text cf="r01_excerpt r45_s r54_phr r55_l" off="815" ext="13">Wast present,</text>
   <end gi="phr" rID="r54_phr" off="828" ext="33"/>
   <text cf="r01_excerpt r45_s r55_l" off="828" ext="1"> </text>
   <start gi="phr" rID="r56_phr" off="829" ext="4"/>
   <text cf="r01_excerpt r45_s r55_l r56_phr" off="829" ext="4">and,</text>
   <end gi="phr" rID="r56_phr" off="833" ext="4"/>
   <text cf="r01_excerpt r45_s r55_l" off="833" ext="1"> </text>
   <start gi="phr" rID="r57_phr" off="834" ext="28"/>
   <text cf="r01_excerpt r45_s r55_l r57_phr" off="834" ext="28">with mighty wings outspread,</text>
   <end gi="phr" rID="r57_phr" off="862" ext="28"/>
   <end gi="l" rID="r55_l" off="862" ext="47"/>
   <text cf="r01_excerpt r45_s" off="862" ext="1">
</text>
   <start gi="l" rID="r58_l" off="863" ext="44">
      <annotation gi="n">Bk.1.21</annotation>
   </start>
   <start gi="phr" rID="r59_phr" off="863" ext="44"/>
   <text cf="r01_excerpt r45_s r58_l r59_phr" off="863" ext="44">Dove-like sat'st brooding on the vast Abyss,</text>
   <end gi="phr" rID="r59_phr" off="907" ext="44"/>
   <end gi="l" rID="r58_l" off="907" ext="44"/>
   <text cf="r01_excerpt r45_s" off="907" ext="1">
</text>
   <start gi="l" rID="r60_l" off="908" ext="42">
      <annotation gi="n">Bk.1.22</annotation>
   </start>
   <start gi="phr" rID="r61_phr" off="908" ext="23"/>
   <text cf="r01_excerpt r45_s r60_l r61_phr" off="908" ext="23">And mad'st it pregnant:</text>
   <end gi="phr" rID="r61_phr" off="931" ext="23"/>
   <text cf="r01_excerpt r45_s r60_l" off="931" ext="1"> </text>
   <start gi="phr" rID="r62_phr" off="932" ext="28"/>
   <text cf="r01_excerpt r45_s r60_l r62_phr" off="932" ext="18">what in me is dark</text>
   <end gi="l" rID="r60_l" off="950" ext="42"/>
   <text cf="r01_excerpt r45_s r62_phr" off="950" ext="1">
</text>
   <start gi="l" rID="r63_l" off="951" ext="40">
      <annotation gi="n">Bk.1.23</annotation>
   </start>
   <text cf="r01_excerpt r45_s r62_phr r63_l" off="951" ext="9">Illumine,</text>
   <end gi="phr" rID="r62_phr" off="960" ext="28"/>
   <text cf="r01_excerpt r45_s r63_l" off="960" ext="1"> </text>
   <start gi="phr" rID="r64_phr" off="961" ext="30"/>
   <text cf="r01_excerpt r45_s r63_l r64_phr" off="961" ext="30">what is low raise and support;</text>
   <end gi="phr" rID="r64_phr" off="991" ext="30"/>
   <end gi="l" rID="r63_l" off="991" ext="40"/>
   <text cf="r01_excerpt r45_s" off="991" ext="1">
</text>
   <start gi="l" rID="r65_l" off="992" ext="43">
      <annotation gi="n">Bk.1.24</annotation>
   </start>
   <start gi="phr" rID="r66_phr" off="992" ext="5"/>
   <text cf="r01_excerpt r45_s r65_l r66_phr" off="992" ext="5">That,</text>
   <end gi="phr" rID="r66_phr" off="997" ext="5"/>
   <text cf="r01_excerpt r45_s r65_l" off="997" ext="1"> </text>
   <start gi="phr" rID="r67_phr" off="998" ext="37"/>
   <text cf="r01_excerpt r45_s r65_l r67_phr" off="998" ext="37">to the height of this great argument,</text>
   <end gi="phr" rID="r67_phr" off="1035" ext="37"/>
   <end gi="l" rID="r65_l" off="1035" ext="43"/>
